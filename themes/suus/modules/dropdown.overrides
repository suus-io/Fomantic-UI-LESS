/*******************************
         Theme Overrides
*******************************/

/* Dropdown Carets */
@font-face {
  font-family: 'Dropdown';
  src:
    url(data:application/x-font-ttf;charset=utf-8;base64,) format('truetype'),
    url(data:application/font-woff;charset=utf-8;base64,) format('woff')
  ;
  font-weight: normal;
  font-style: normal;
}

.ui.dropdown .menu > .item {
  font-size: @itemFontSize !important;
}

.ui.dropdown, ui.dropdown.inline {
  > .dropdown.icon {
    font-family: 'Icons';
    line-height: 1;
    height: 1em;
    width: 1.23em;
    backface-visibility: hidden;
    font-weight: normal;
    font-style: normal;
    text-align: center;
    bottom: 0.5em;
    vertical-align: middle;
    margin-left: 1em !important;
  }
}
.ui.inline.dropdown .dropdown.icon {
  vertical-align: super;
}

.ui.dropdown.button {
  text-transform: none;
}

.ui.form {
  .field.error {
    .ui.dropdown {
      &.selection.active {
        .menu {
          border-color: @lightRed;
        }
      }
      box-shadow: 7px 7px 0px -6px @lightRed, -7px 7px 0px -6px @lightRed;
      &.inline {
        box-shadow: none;
      }
    }
  }
}


.ui.input .floating.button {
  top: 0;
  bottom: 5px;
  & > .text {
    font-size: 0.85714286em;
    font-weight: bold;
    line-height: 12px;
    display: inline-flex;
  }
}

.ui.dropdown.button {
  & > .default.text, & > .text {
    font-weight: 900 !important;
  }
}
.ui.dropdown {
/*
  &.search.multiple > input.search {
    font-size: @labelSize !important;
      background: #3283f7 !important;
      color: white !important;
      font-weight: 900 !important;
      text-transform: uppercase !important;
      padding: @labelPadding !important;
      border-radius: 3px !important;
      margin: @labelMargin !important;
      box-shadow: @labelBoxShadow !important;
  }*/
  & > .default.text, & > .text {
    font-size: @relative18px;
    font-weight: 300 !important;
    line-height: @inputLineHeight;
    display: inline-flex;
    white-space: nowrap;
    overflow: hidden;
  }
  & > .dropdown.icon {
    width: auto;
  }
  & > .dropdown.icon:before {
    content: '\eeb8';
  }
  .search {
    font-size: @relative18px;
  }
  &.multiple {
    .sizer {
      font-size: @itemFontSize;
    }
    .ui.label {
      .text {
        display: inline;
      }
      .description {
        display: none;
      }
    }
  }
  &.allow-additions {
    .ui.search {
      font-size: @relative12px;
    }
  }
  .menu {
    .item {
      display: flex;
      .text {
        flex-grow: 1;
      }
    }
  }
}

.ui.dropdown.inline {
  & > .default.text, & > .text {
    //font-size: inherit;
  }
}

/* Sub Menu */
.ui.dropdown .menu .item .dropdown.icon:before {
  content: '\eeba'/*rtl:'\f0d9'*/;
}
.ui.dropdown .menu > .item > .icon {
  margin-top: 0 !important;
}
.ui.dropdown .item .left.dropdown.icon:before,
.ui.dropdown .left.menu .item .dropdown.icon:before {
  content: "\eeba"/*rtl:"\f0da"*/;
}

/* Vertical Menu Dropdown */
.ui.vertical.menu .dropdown.item > .dropdown.icon:before {
  content: "\eeba"/*rtl:"\f0d9"*/;
}

/* Icons for Reference
.dropdown.down.icon {
  content: "\f0d7";
}
.dropdown.up.icon {
  content: "\f0d8";
}
.dropdown.left.icon {
  content: "\f0d9";
}
.dropdown.icon.icon {
  content: "\f0da";
}
*/

.ui.menu {
  .ui.dropdown.link {
    background: transparent;
    padding: 0 !important;
    .text {
      text-transform: none;
      padding: 8px 16px;
      color: @white;
    }
    &.visible {
      background: white;
      border-radius: @3px @3px 0 0;
      .text {
        color: @black;
      }
      .menu {
        border: 0px;
        margin-top: 0px !important;
        box-shadow: rgba(0, 0, 0, 0.243) 0px 1px 4px !important;
        border-radius: 0px 0px @3px @3px !important;
        & > .item {
          margin: @8px;
        }
      }
    }
  }
}

.ui.search.dropdown .menu {
  max-height: @scrollingComputerMaxMenuHeight;
}

.ui.selection.upward.dropdown {
  &, &:hover {
    box-shadow: 7px 7px 0px -6px #bbbbbb, -7px 7px 0px -6px #bbbbbb !important;
  }
}
.ui.active.selection.upward.dropdown {
  &, &:hover {
    box-shadow: 7px 7px 0px -6px #3283f7, -7px 7px 0px -6px #3283f7 !important;
  }
}
