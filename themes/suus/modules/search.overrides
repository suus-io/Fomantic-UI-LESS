/*******************************
         Theme Overrides
*******************************/


@keyframes wiggle {
  0% {transform: rotate(0deg);}
  25% {transform: rotate(-10deg);}
  50% {transform: rotate(0deg);}
  75% {transform: rotate(10deg);}
  100% {transform: rotate(0deg);}
}

.search.form .icon.input .icon:before {
  animation: wiggle 1s ease-in-out 3;
}