/*******************************
         Theme Overrides
*******************************/


.ui.horizontal.divider {
  &:before, &:after {
    background-image: url('data:image/png;base64,');
  }
  &:before {
    background-position: right 1em center 0;
  }
  &:after {
    background-position: left 1em center 0;
  }
  &.left {
    color: #616161;
    font-size: 14px;
    font-weight: bold;
    line-height: 19px;
    margin: @12px 0;
    padding-top: 1rem;
    margin-bottom: -@4px;
    display: flex;
    .ui.icon {
      vertical-align: sub;
      margin-right: @28px;
      margin-left: @25px;
      font-size: @16px;
    }
    &:before {
      content: unset;
    }
    &:after {
      width: 100%;
      background-repeat: repeat-x;
      background-position: left 1em top 50%;
      left: @16px;
    }
  }
}
.ui.horizontal.divider.left + .description {
  margin: 0.5rem 0 -0.5rem 5.5rem;
  color: #888;
  max-width: 1000px;
}

.ui.header {
  &.dividing {
    width: 100%;
    padding-bottom: 0.5rem;
  }
  .icon:only-child {
    margin: 0 !important;
  }
}

@media only screen and (max-width : (@tabletBreakpoint - 1px)) {
  .ui.stackable.grid .ui.vertical.divider:before,
  .ui.grid .stackable.row .ui.vertical.divider:before,
  .ui.stackable.grid .ui.vertical.divider:after,
  .ui.grid .stackable.row .ui.vertical.divider:after {
    background-image: url('data:image/png;base64,');
  }
}
